{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/extensions": [0, { "js": "always" }],
    "import/prefer-default-export": "off",
    "object-curly-newline": "off",
    "implicit-arrow-linebreak": "off",
    "comma-dangle": ["error", { "functions": "never" }],
    "no-underscore-dangle": "off",
    "no-console": "off"
  }
}
